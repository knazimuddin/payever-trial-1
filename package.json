{
  "scripts": {
    "start": "ts-node src/main.ts",
    "server": "nodemon",
    "start:migration:dev": "nodemon --watch src --exec 'ts-node src/migration-main.ts'",
    "cli": "ts-node src/cli.ts",
    "test": "jest --config=jest.json",
    "test:watch": "jest --watch --config=jest.json",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "e2e": "jest --config=e2e/jest-e2e.json --forceExit",
    "e2e:watch": "jest --watch --config=e2e/jest-e2e.json",
    "pre-commit": "npm run tslint:fix && npm run test && npm run e2e",
    "tslint": "tslint --project tsconfig.json",
    "tslint:fix": "tslint --project tsconfig.json --fix",
    "migrations": "./node_modules/.bin/db-migrate"
  },
  "dependencies": {
    "@nestjs/common": "^5.5.0",
    "@nestjs/core": "^5.5.0",
    "@nestjs/microservices": "^5.5.0",
    "@nestjs/mongoose": "^5.2.2",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/testing": "^5.5.0",
    "@nestjs/typeorm": "^5.2.2",
    "@nestjs/websockets": "^5.4.0",
    "@pe/nest-kit": "3.0.1",
    "@pe/notifications-sdk": "^1.0.0",
    "@types/mongoose": "^5.3.7",
    "@types/uuid": "^3.4.4",
    "class-transformer": "^0.1.10",
    "class-validator": "^0.8.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.5",
    "db-migrate-mongodb": "^1.5.0",
    "db-migrate-plugin-typescript": "^2.0.0",
    "lodash": "^4.17.11",
    "lodash-es": "^4.17.11",
    "moment": "^2.24.0",
    "mongoose": "^5.4.4",
    "mysql": "^2.16.0",
    "nestjs-command": "0.0.4",
    "nodemon": "^1.18.9",
    "php-serialize": "^2.1.0",
    "php-unserialize": "0.0.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.4.0",
    "typeorm": "^0.2.11",
    "typescript": "^3.2.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/node": "^7.10.2",
    "jest": "^24.1.0",
    "supertest": "^3.3.0",
    "ts-jest": "^24.0.0",
    "ts-loader": "^4.5.0",
    "ts-node": "^6.0.0",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.12.1"
  }
